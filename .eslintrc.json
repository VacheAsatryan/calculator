{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "plugins": ["html"],
    "settings": {
        "html/indent": "0"
        
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars":"off",
        "prefer-const":"off",
        "no-eval":"off"
        
    }
}
